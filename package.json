{
  "name": "metaface",
  "version": "1.0.0",
  "description": "A face detection application using Clarifai API using React and Express",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gsbakshi/metaface.git"
  },
  "keywords": [],
